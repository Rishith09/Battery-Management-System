function [a,b] = fcn(x1,x2,x3)
x1=uint16(x1);
x2=uint16(x2);
x3=uint16(x3);
if (x1~=x2)
    if (x1>x2)
        a=1;
    elseif (x1<x2)
        a=-1;
    else 
        a=0;
    end
else
    a=0;
end
if (x2~=x3)   
    if (x2>x3)
        b=1;
    elseif(x2<x3)
        b=-1;
    else
         b=0;
      end
  else
     b=0;
end
end
